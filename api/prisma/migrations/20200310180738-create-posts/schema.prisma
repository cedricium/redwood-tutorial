datasource DS {
  provider = "sqlite"
  url = "***"
}

generator photonjs {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

// Define your own datamodels here and run `yarn redwood db save` to create
// migrations for them.
model Post {
  id          Int @id @default(autoincrement())
  title       String
  body        String
  createdAt   DateTime @default(now())
}
